{
  "name": "kawlo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:debug": "react-native run-android --active-arch-only --appId com.kawlo.debug",
    "android:release": "react-native run-android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "android:apk": "cd android &&./gradlew clean && ./gradlew assembleRelease",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "android-dev": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "tsc && yarn run lint",
    "tsc": "tsc",
    "jest": "jest",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --config ./.prettierrc.js --write 'app/**/**/*.{ts,tsx,json}'",
    "watch-need-help": "echo 256 | sudo tee -a /proc/sys/fs/inotify/max_user_instances && echo 32768 | sudo tee -a /proc/sys/fs/inotify/max_queued_events && echo 65536 | sudo tee -a /proc/sys/fs/inotify/max_user_watches && watchman shutdown-server",
    "prettier:fix-schema": "prettier --parser graphql --write '**.graphql'",
    "apollo:download": "apollo client:download-schema -c apollo.config.js schema.graphql",
    "apollo:download:wp": "apollo client:download-schema -c apollo-wp.config.js schema-wp.graphql",
    "apollo:generate": "apollo client:codegen -c apollo.config.js --target typescript --globalTypesFile ./src/globalTypes.ts --passthroughCustomScalars --customScalarsPrefix GraphQL_ && yarn run prettier:fix-schema",
    "apollo:generate:wp": "apollo client:codegen -c apollo-wp.config.js --target typescript --globalTypesFile ./src/globalTypes.ts --passthroughCustomScalars --customScalarsPrefix GraphQL_ && yarn run prettier:fix-schema"
  },
  "dependencies": {
